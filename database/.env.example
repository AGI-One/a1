# ERPNext Database Environment Variables (Example)
# Copy this file to .env and update the values

# MariaDB Configuration
MARIADB_ROOT_PASSWORD=erpnext_mariadb_root_password
MARIADB_DATABASE=erpnext
MARIADB_USER=erpnext
MARIADB_PASSWORD=erpnext_mariadb_password

# Redis Configuration (leave empty for development, set for production)
REDIS_PASSWORD=erpnext_redis_password

# Network
DB_HOST=mariadb
REDIS_HOST=redis

# Ports
DB_PORT=3306
REDIS_PORT=6379
